    <RootNamespace>Microsoft.Maui.Controls.Android</RootNamespace>
    <AssemblyName>Microsoft.Maui.Controls.Android.FormsViewGroup</AssemblyName>
    <OutputType>Library</OutputType>
    <Nullable>disable</Nullable>
